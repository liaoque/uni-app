
@charset "UTF-8";
.fiex-one {
	width: 172.50rpx;
	text-align: center;
}
.fiex-two {
	width: 345rpx;
}
.square {
	height: 88rpx;
	width: 88rpx;
	line-height: 88rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.face-panel .face {
	width: 88rpx;
	-webkit-justify-content: center;
	-webkit-box-pack: center;
	        justify-content: center;
	-webkit-align-items: center;
	-webkit-box-align: center;
	        align-items: center;
}

/* 头条小程序组件内不能引入字体 */
page {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	box-sizing: border-box;
	background-color: #efeff4;
	min-height: 100%;
	height: auto;
}
view {
	font-size: 14px;
	line-height: inherit;
}
.content-box {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	height: 44px;
	line-height: 44px;
	padding: 0 15px;
	position: relative;
	background-color: #fff;
	border-bottom-color: #f5f5f5;
	border-bottom-width: 1px;
	border-bottom-style: solid;
}
.content-text {
	font-size: 15px;
	color: #6E6E6E;
}
.slot-button-text {
	color: #ffffff;
	font-size: 14px;
}

